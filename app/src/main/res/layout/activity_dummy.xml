<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.abhijeetDeshmukh.DummyActivity">

    <TextView android:id="@+id/tv_file1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="File : 1"
        android:layout_marginLeft="58dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="46dp" />

    <TextView android:id="@+id/tv_file1_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" "
        app:layout_constraintLeft_toRightOf="@+id/tv_file1"
        android:layout_marginLeft="76dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="46dp" />

    <TextView android:id="@+id/tv_file2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="File : 2"
        android:layout_marginLeft="58dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="23dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_file1" />

    <TextView android:id="@+id/tv_file2_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" "
        app:layout_constraintLeft_toRightOf="@+id/tv_file2"
        android:layout_marginLeft="76dp"
        android:layout_marginTop="23dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_file1_name" />

    <TextView android:id="@+id/tv_analysis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Analysis"
        app:layout_constraintLeft_toRightOf="@+id/tv_file2"
        android:layout_marginLeft="-58dp"
        android:layout_marginTop="39dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_file2_name" />

    <TextView android:id="@+id/tv_kl_divergence"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="KL Divergance : "
        android:layout_marginTop="37dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_analysis"
        app:layout_constraintLeft_toRightOf="@+id/tv_analysis"
        android:layout_marginLeft="0dp" />

    <TextView android:id="@+id/tv_kld_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" "
        app:layout_constraintLeft_toRightOf="@+id/tv_kl_divergence"
        android:layout_marginLeft="21dp"
        android:layout_marginTop="37dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_analysis" />

    <TextView android:id="@+id/tv_sad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Spectral angle distance : "
        android:layout_marginRight="4dp"
        app:layout_constraintRight_toRightOf="@+id/tv_kl_divergence"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_kl_divergence" />

    <TextView android:id="@+id/tv_sad_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" "
        android:layout_marginTop="19dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_kld_value"
        app:layout_constraintLeft_toRightOf="@+id/tv_kl_divergence"
        android:layout_marginLeft="21dp" />

    <TextView android:id="@+id/tv_sid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Spectral Information Divergence"
        android:layout_marginTop="22dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_sad"
        android:layout_marginRight="-3dp"
        app:layout_constraintRight_toRightOf="@+id/tv_sad" />

    <TextView android:id="@+id/tv_sid_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" "
        app:layout_constraintLeft_toRightOf="@+id/tv_sid"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="23dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_sad_value" />

    <TextView
        android:id="@+id/tv_kld_pq"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="KLD pq"
        app:layout_constraintLeft_toRightOf="@+id/tv_kl_divergence"
        android:layout_marginLeft="19dp"
        android:layout_marginTop="11dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_analysis" />

    <TextView
        android:id="@+id/tv_kld_qp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="KLD qp"
        app:layout_constraintLeft_toRightOf="@+id/tv_kld_pq"
        android:layout_marginLeft="133dp"
        app:layout_constraintBottom_toBottomOf="@+id/tv_kld_pq"
        android:layout_marginBottom="-9dp" />

    <TextView
        android:id="@+id/tv_kld_qp_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" "
        app:layout_constraintTop_toTopOf="@+id/tv_kld_qp"
        android:layout_marginTop="26dp"
        app:layout_constraintLeft_toRightOf="@+id/tv_kld_value"
        android:layout_marginLeft="172dp" />

</android.support.constraint.ConstraintLayout>
